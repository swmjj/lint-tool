    "@types/text-table": "^0.2.2",
    "copyfiles": "^2.4.1",
    "del": "^7.0.0",
    "delete": "^1.1.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  }
}
